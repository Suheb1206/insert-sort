#include<iostream>
using namespace std;

void insertsort(int A[],int n)
{
	int x,i,j;
	for(i=1;i<n;i++)
	{
		j=i-1;
		x=A[i];
		while(j>-1&&A[j]>x)
		{
			A[j+1]=A[j];
			j--;
		}
		A[j+1]=x;
	}
}
int main()
{
    int arr[] = { 14, 10, 12, 2, 4 };
    int n = sizeof(arr) / sizeof(arr[0]);
 
    insertSort(arr, n);
    
    	for(i=0;i<n;i++)
	    {
	  	  cout<<A[i]<<" ";
	    }
 
    return 0;
}
